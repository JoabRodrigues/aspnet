@page
@model ConsultarModel
@{
    ViewData["Title"] = "Consultar";
}

<h2>Consultar</h2>

<form method="post">
    <div class="form-group">
        <label>Cpf</label>
        <input type="text" id="Cpf" name="Cpf"/>
    </div>
    <button>Buscar</button>
</form>
@if(ViewData["Usuario"] != null)
{
    <div class="card">
        <div class="card-body">
            @(((FirstProject.Data.Usuario)ViewData["Usuario"]).Email)
        </div>
    </div>
}